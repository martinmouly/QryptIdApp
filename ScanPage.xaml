<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="QryptIdApp.ScanPage">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="mycode" Text="" TextColor="Red" HorizontalOptions="Center"/>
            <Button x:Name="Button1" Text="Scan" Clicked="Button_Clicked" HorizontalOptions="Center" VerticalOptions="CenterAndExpand" Margin="0,0,0,0" />
            <Button Text="Scan" Clicked="Button2_Clicked" VerticalOptions="EndAndExpand" HorizontalOptions="Center" x:Name="Button2"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>